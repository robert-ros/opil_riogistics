<?xml version="1.0"?>
<launch>

    <!-- Launch modbus communication -->
    <node pkg="robotnik_modbus_io" type="robotnik_modbus_io_node" name="robotnik_modbus_io" output="screen">
        <param name="ip_address" type="string" value="192.168.1.100" />
        <param name="port" value="502"/>
        <param name="digital_inputs_addr" value="0" />
        <param name="digital_outputs_addr" value="0"/>  
        <param name="digital_inputs" value="9"/>
        <param name="digital_outputs" value="1"/>
        <param name="big_endian" value="false"/>
    </node>


</launch>